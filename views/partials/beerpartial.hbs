<div id="template">

  <div id="card">

    <img class="beerImage" src={{this.image_url}}/>
    <div class="card-body">
        <h3 class="card-title">{{this.name}}</h3>
        <h5 class="card-tag">{{this.tagline}}</h5>
        <p class="card-text">{{this.description}}</p>
    </div>
    
  </div>
</div>